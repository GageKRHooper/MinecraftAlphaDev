{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Minecraft Alpha Client",
      "request": "launch",
      "mainClass": "org.mcphackers.launchwrapper.Launch",
      "vmArgs": "-Djava.library.path=${workspaceFolder}/libraries/natives",
      "args": [
        "--username", "Player",
        "--session", "-",
        "--version", "a1.1.2_01",
        "--gameDir", "${workspaceFolder}/minecraft/game",
        "--assetsDir", "${workspaceFolder}/minecraft/game/assets",
        "--assetIndex", "a1.0.14",
        "--userType", "legacy"
      ],
      "classPaths": [
        "${workspaceFolder}/minecraft/jars/deobfuscated.jar",
        "${workspaceFolder}/libraries/*"
      ]
    },
    {
      "type": "java",
      "name": "Minecraft Server",
      "request": "launch",
      "mainClass": "org.mcphackers.launchwrapper.Launch",
      "args": [
        "--nogui"
      ],

    "classPaths": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/bin",
                "${workspaceFolder}/libraries/net/java/jinput/jinput/2.0.5/jinput-2.0.5.jar",
                "${workspaceFolder}/libraries/net/java/jutils/jutils/1.0.0/jutils-1.0.0.jar",
                "${workspaceFolder}/libraries/org/lwjgl/lwjgl/lwjgl/2.9.4-nightly-20150209/lwjgl-2.9.4-nightly-20150209.jar",
                "${workspaceFolder}/libraries/org/lwjgl/lwjgl/lwjgl_util/2.9.4-nightly-20150209/lwjgl_util-2.9.4-nightly-20150209.jar",
                "${workspaceFolder}/libraries/org/lwjgl/lwjgl/lwjgl-platform/2.9.4-nightly-20150209/lwjgl-platform-2.9.4-nightly-20150209.jar",
                "${workspaceFolder}/libraries/com/paulscode/codecjorbis/20230120/codecjorbis-20230120.jar",
                "${workspaceFolder}/libraries/com/paulscode/codecwav/20101023/codecwav-20101023.jar",
                "${workspaceFolder}/libraries/com/paulscode/libraryjavasound/20101123/libraryjavasound-20101123.jar",
                "${workspaceFolder}/libraries/com/paulscode/librarylwjglopenal/20100824/librarylwjglopenal-20100824.jar",
                "${workspaceFolder}/libraries/com/paulscode/soundsystem/20120107/soundsystem-20120107.jar",
                "${workspaceFolder}/libraries/org/mcphackers/launchwrapper/1.1.3/launchwrapper-1.1.3.jar",
                "${workspaceFolder}/libraries/org/json/json/20230311/json-20230311.jar",
                "${workspaceFolder}/libraries/org/ow2/asm/asm/9.7/asm-9.7.jar",
                "${workspaceFolder}/libraries/org/ow2/asm/asm-tree/9.7/asm-tree-9.7.jar",
                "${workspaceFolder}/minecraft_server/jars/deobfuscated.jar"
            ]
